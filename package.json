{
  "name": "prunaverso-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/prestart.cjs && vite",
    "dev-unsafe": "vite",
    "build": "vite build",
    "build-local": "vite build --mode dev",
    "serve-characters": "node scripts/save_checkpoint_server.cjs",
    "generate-characters-index": "node scripts/generate_characters_index.cjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky install",
    "health-check": "node scripts/health-check.cjs",
    "monitor": "node scripts/monitor.cjs",
    "prestart": "node scripts/prestart.cjs",
    "dev-safe": "npm run prestart && npm run dev-unsafe",
    "recovery": "node scripts/health-check.cjs && npm run dev-unsafe",
    "diagnostic": "node scripts/quick-diagnostic.cjs",
    "diagnose-ui": "node scripts/diagnose-ui.cjs",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy-manual": "npm run build && git add dist -f && git commit -m 'Deploy to GitHub Pages' && git subtree push --prefix dist origin gh-pages",
    "deploy-local": "node scripts/deploy.cjs",
    "deploy-hybrid": "node scripts/deploy-hybrid.cjs",
    "setup-github": "node scripts/setup-github.cjs",
    "sync-check": "node scripts/sync-check.cjs",
    "manifiesto": "python scripts/prunaverso_manifiesto.py"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "classnames": "^2.5.1",
    "daisyui": "^5.1.27",
    "express": "^5.1.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.545.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.2.1",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "husky": "^8.0.0",
    "terser": "^5.44.0",
    "vite": "^7.1.7"
  },
  "optionalDependencies": {
    "json5": "^2.2.3"
  }
}
